#include<iostream>
using namespace std;

int main() {
	int L1, L2, L3;
	cout<<"digite o valor de L1"<<endl;
	cin>>L1;
	cout<<"digite o valor de L2"<<endl;
	cin>>L2;
	cout<<"digite o valor de L3"<<endl;
	cin>>L3;
	
	if (L1 == L2 && L2 == L3 && L1 == L3) {
	
	cout<<"o triangulo eh equilatero"<<endl;
}

	else if (L1 != L2 && L2 != L3 && L1 != L3) {
	
	cout<<"o trinagulo eh escaleno"<<endl;
	} 
	
	else 
	cout<<"o triangulo eh isoceles"<<endl;

	return 0;
}
